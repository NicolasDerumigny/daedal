######
# Setting the targets to build
# 

# Targets to build
INDIR_COUNT=25
GRAN_COUNT=1

ENABLE_STATS=no # yes

TM_INDIRCOUNT_SUFFIX=tm-indir$(INDIR_COUNT)
TM_GRAN_SUFFIX=tm-gran
TM_GRANCOUNT_SUFFIX=$(TM_GRAN_SUFFIX)$(GRAN_COUNT)

ORIGINAL_SUFFIX=original
CAE_SUFFIX=tm-cae
TM_ARG_SUFFIX=tm-arg
TM_DAE_SUFFIX=tm-dae
TM_FULL_DAE_SUFFIX=tm-full-dae

# Targets
ORIGINAL_TARGETS=$(BENCHMARK).$(ORIGINAL_SUFFIX)
CAE_TARGETS=$(BENCHMARK).$(CAE_SUFFIX) $(BENCHMARK).$(CAE_SUFFIX)2
TM_ARG_TARGETS=$(BENCHMARK).$(TM_ARG_SUFFIX)
TM_DAE_TARGETS=$(BENCHMARK).$(TM_DAE_SUFFIX)
TM_FULL_DAE_TARGETS=$(BENCHMARK).$(TM_FULL_DAE_SUFFIX)

ALLTARGETS= $(ORIGINAL_TARGETS) $(CAE_TARGETS) $(TM_ARG_TARGETS) $(TM_DAE_TARGETS) $(TM_FULL_DAE_TARGETS)

# Output directory
BINDIR=../bin

# TARGETS
TARGETS=$(addprefix $(BINDIR)/, $(ALLTARGETS))



